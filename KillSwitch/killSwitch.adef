sandboxed: true
start: manual

executables:
{
    killSwitch = ( killSwitch )
    canOpen = ( canOpen )
}

processes:
{
    run:
    {
        ( killSwitch )
        ( canOpen )
    }

    faultAction: restart
}

bundles:
{
    file:
    {
        [x] script/can-init.sh /bin/
    }
}

bindings:
{
    killSwitch.killSwitch.dataRouter -> dataRouter.dataRouter
    killSwitch.killSwitch.mangoh_canOpenIox1 -> canOpen.canOpen.mangoh_canOpenIox1
}

