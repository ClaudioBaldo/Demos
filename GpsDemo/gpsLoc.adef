sandboxed: false

version: 1.0.0
maxFileSystemBytes: 512K

executables:
{
        gpsdemo = ( gpsdemoComponent )
}

processes:
{
    envVars:
    {
        LE_LOG_LEVEL = DEBUG
    }
    run:
    {
        ( gpsdemo )
    }
    faultAction: stopApp
}

bindings:
{
    gpsdemo.gpsdemoComponent.le_posCtrl -> positioningService.le_posCtrl
    gpsdemo.gpsdemoComponent.le_pos -> positioningService.le_pos
    gpsdemo.gpsdemoComponent.dataRouter -> dataRouter.dataRouter
}
